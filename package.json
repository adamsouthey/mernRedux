    "license": "MIT",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "config": "^3.1.0",
        "express": "^4.16.4",
        "express-validator": "^5.3.1",
        "gravatar": "^1.8.0",
        "jsonwebtoken": "^9.0.0",
        "mongoose": "^5.5.1",
        "request": "^2.88.0"
    },
    "devDependencies": {
        "@testing-library/cypress": "^5.0.2",
        "@testing-library/react": "^9.3.0",
        "concurrently": "^4.1.0",
        "cypress": "^3.4.1",
        "nodemon": "^1.18.11"
    }
}